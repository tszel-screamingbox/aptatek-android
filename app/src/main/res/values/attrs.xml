<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LevelIndicatorView">
        <attr name="indicatorColor" format="color" />
    </declare-styleable>

    <declare-styleable name="RangeInfoRowView">
        <attr name="backgroundColor" format="color" />
    </declare-styleable>

    <declare-styleable name="HeaderView">
        <attr name="titleText" format="string" />
        <attr name="subTitleText" format="string" />
    </declare-styleable>

    <declare-styleable name="BatteryView">
        <attr name="percentage" format="integer" />
    </declare-styleable>

    <declare-styleable name="BubbleTextView">
        <attr name="circleColor" format="color" />
        <attr name="fillColor" format="color" />
        <attr name="circleWidth" format="dimension" />
        <attr name="textPrimary" format="string" />
        <attr name="textSecondary" format="string" />
        <attr name="textColor" format="color" />
    </declare-styleable>

</resources>